#include <tunables/global>

/usr/bin/irssi {
  #include <abstractions/base>
  #include <abstractions/openssl>

  /etc/gai.conf r,
  /etc/host.conf r,
  /etc/hosts r,
  /etc/nsswitch.conf r,
  /etc/resolv.conf r,
  /lib/x86_64-linux-gnu/ld-*.so mr,
  /usr/bin/irssi mr,
  /usr/share/perl/5.26.2/* r,
  /usr/share/perl/5.26.2/** r,
  owner /home/*/.irssi/** rw,
  /usr/share/irssi/** r,
  /usr/share/ca-certificates/** r,
  /usr/share/perl/5.28.1/** r,
}
